.vdivide {
  width: 23px;
  border: 1px solid #666666;
  border-width: 0 1px 0 1px;
  border-right-color: rgba(0, 0, 0, 0.3);
  background: #494949;
  background: url("/img/draghandle_v.png"), -webkit-gradient(linear, left top, right top, from($dark-from-gradient), to($dark-to-gradient));
  background: url("/img/draghandle_v.png"), -webkit-linear-gradient(left, $dark-from-gradient, $dark-to-gradient);
  background: url("/img/draghandle_v.png"), -moz-linear-gradient(left, $dark-from-gradient, $dark-to-gradient);
  background: url("/img/draghandle_v.png"), linear-gradient(left, $dark-from-gradient, $dark-to-gradient);
  background-repeat: no-repeat;
  background-position: center;
  box-sizing:border-box;
  cursor: pointer;
}
.hdivide {
  height: 23px;
  border: 1px solid #666666;
  border-width: 1px 0 1px 0;
  border-bottom-color: rgba(0, 0, 0, 0.7);
  background: #494949;
  background: url("/img/draghandle_h.png"), -webkit-gradient(linear, left top, left bottom, from($dark-from-gradient), to($dark-to-gradient));
  background: url("/img/draghandle_h.png"), -webkit-linear-gradient(top $dark-from-gradient, $dark-to-gradient);
  background: url("/img/draghandle_h.png"), -moz-linear-gradient(top, $dark-from-gradient, $dark-to-gradient);
  background: url("/img/draghandle_h.png"), linear-gradient(top, $dark-from-gradient, $dark-to-gradient);
  background-repeat: no-repeat;
  background-position: center;
  box-sizing:border-box;
  font-size: 1em;
  cursor: pointer;
}
