@-webkit-keyframes pulsate {
    from {
      opacity: 0.5;
    }
    50% {
      opacity: 1.0;
    }
    to {
      opacity: 0.5;
    }
}
@-moz-keyframes pulsate {
    0% {
      opacity: 0.5;
    }
    50% {
      opacity: 1.0;
    }
    100% {
      opacity: 0.5;
    }
}
@-ms-keyframes pulsate {
    0% {
      opacity: 0.5;
    }
    50% {
      opacity: 1.0;
    }
    100% {
      opacity: 0.5;
    }
}
@-o-keyframes pulsate {
    0% {
      opacity: 0.5;
    }
    50% {
        opacity: 1.0;
    }
    100% {
      opacity: 0.5;
    }
}
@keyframes pulsate {
    from {
      opacity: 0.5;
    }
    50% {
      opacity: 1.0;
    }
    to {
      opacity: 0.5;
    }
}
